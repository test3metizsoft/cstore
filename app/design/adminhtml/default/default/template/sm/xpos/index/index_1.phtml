    function iLog(name, value, levels) {
        var isLog = $.jStorage.get('enable_status', 'false');
        var color = 'green';
        var prefixName = ''
        switch (levels) {
            case 1:
                /*notice*/
                color = 'green';
                prefixName = '>';
                break;
            case 2:
                /*major*/
                prefixName = '??';
                color = 'blue';
                break;
            case 5:
                /*error*/
                prefixName = '!!!!!';
                color = 'red';
                break;
            default :
                color = 'green';
                prefixName = '>';
                break;
        }
        name = prefixName + ' ' + name;
        if (isLog == 1 || isLog == '1') {
            if (typeof value == 'undefined' || value == null) {
                console.log('%c ' + name, 'color: ' + color + '; font-weight: bold;');
            } else {
                console.log('%c ' + name + '=' + value, 'color: ' + color + '; font-weight: bold;');
            }
        }
