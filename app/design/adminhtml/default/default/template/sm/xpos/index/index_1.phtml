            timeout: 30000,
            success: function (data) {
                if (jQuery('#network-status').val() != 'online') {
                    alert('The X-POS is now back in online mode. You can resend any pending orders through the Sidebar menu.');

                    jQuery('#network-status').val('online');

                    goOnline();
                }
                networkCheck = false;
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                networkCheck = false;
                if (isAvailability()) {
                    jQuery.ajax({
                        type: 'GET',
                        url: '<?php echo $this->getUrl('adminhtml/xpos/checkNetwork') ?>',
                        timeout: 30000,
                        success: function (data) {
                            if (jQuery('#network-status').val() != 'online') {
                                alert('The X-POS is now back in online mode. You can resend any pending orders through the Sidebar menu.');
                                jQuery('#network-status').val('online');
                                goOnline();
                            }
                            networkCheck = false;
                        },
                        error: function (XMLHttpRequest, textStatus, errorThrown) {
                            networkCheck = false;
                            if (isAvailability()) {
                                jQuery.ajax({
                                    type: 'GET',
                                    url: '<?php echo $this->getUrl('adminhtml/xpos/checkNetwork') ?>',
                                    timeout: 30000,
                                    success: function (data) {
                                        if (jQuery('#network-status').val() != 'online') {
                                            alert('The X-POS is now back in online mode. You can resend any pending orders through the Sidebar menu.');
                                        }
                                        jQuery('#network-status').val('online');
                                        networkCheck = false;
                                    },
                                    error: function (XMLHttpRequest, textStatus, errorThrown) {
                                        jQuery('#network-status').val('offline');
                                        goOffline();
                                        networkCheck = false;
                                    }
                                });
                            }
                        }
                    });
                } else {
                    //still offline
                }
            }
        });
    }

